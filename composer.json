{
  "name": "jambagecom/base-css",
  "type": "typo3-cms-extension",
  "description": "This provides the PHP-CSS-Parser.",
  "keywords": [
    "TYPO3",
    "extension",
    "parser",
    "css",
    "stylesheet"
  ],
  "support": {
    "issues": "https://github.com/franzholz/base_css/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
        "name": "Raphael Schweikert"
    },
    {
      "name": "Franz Holzinger",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "php": ">=5.3.2",
    "typo3/cms-core": ">=6.2.0,<9.99.0"
  },
  "autoload": {
    "psr-0": {
        "Sabberworm\\CSS": "Vendor/PHP-CSS-Parser/lib/"
    }
  },
  "replace": {
    "base_css": "self.version",
    "typo3-ter/base-css": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "base_css"
    }
  }
}
